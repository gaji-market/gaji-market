{
	"name": "front",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@reduxjs/toolkit": "^1.8.6",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.4.2",
		"react-scripts": "5.0.1",
		"sass": "^1.55.0",
		"styled-components": "^5.3.6",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"rules": {
			"semi": [
				"error",
				"always"
			],
			"quotes": [
				"error",
				"single"
			],
			"comma-dangle": [
				"error",
				"always-multiline"
			],
			"indent": [
				"error",
				2
			],
			"arrow-spacing": [
				"error",
				{
					"before": true,
					"after": true
				}
			],
			"brace-style": [
				"error",
				"1tbs",
				{
					"allowSingleLine": false
				}
			],
			"comma-spacing": [
				"error",
				{
					"before": false,
					"after": true
				}
			],
			"func-call-spacing": [
				"error",
				"never"
			],
			"key-spacing": [
				"error",
				{
					"beforeColon": true,
					"afterColon": true
				}
			],
			"keyword-spacing": [
				"error",
				{
					"before": true,
					"after": true
				}
			],
			"semi-spacing": [
				"error",
				{
					"before": false,
					"after": false
				}
			],
			"space-before-blocks": [
				"error",
				"always"
			],
			"jsx-quotes": [
				"error",
				"prefer-single"
			]
		},
		"browserslist": {
			"production": [
				">0.2%",
				"not dead",
				"not op_mini all"
			],
			"development": [
				"last 1 chrome version",
				"last 1 firefox version",
				"last 1 safari version"
			]
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
